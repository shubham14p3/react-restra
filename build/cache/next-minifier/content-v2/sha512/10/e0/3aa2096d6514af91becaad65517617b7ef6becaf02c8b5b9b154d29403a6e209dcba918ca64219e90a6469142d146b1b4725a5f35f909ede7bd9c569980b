\n      return _objectSpread(_objectSpread({}, state), {}, {\n        isFileDialogActive: false,\n        isDragActive: false,\n        draggedFiles: [],\n        acceptedFiles: [],\n        fileRejections: []\n      });\n\n    default:\n      return state;\n  }\n}"]}}